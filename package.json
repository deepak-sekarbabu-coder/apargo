{
  "name": "apargo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "start": "next start",
    "prebuild": "tsx scripts/pre-build-exclude-routes.ts exclude",
    "build": "npm run replace-sw-env && next build",
    "postbuild": "tsx scripts/post-build-cleanup.ts && tsx scripts/pre-build-exclude-routes.ts restore",
    "export": "next export",
    "netlify-build": "npx rimraf .next .netlify && node scripts/netlify-optimize.cjs && npm run build",
    "test": "jest --config jest.config.cjs",
    "test:payment-category": "tsx tests/payments/test-payment-category.ts",
    "test:payment-events": "tsx scripts/test-payment-events.ts",
    "test:payment-scheduler": "tsx scripts/test-payment-scheduler.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "accessibility-check": "tsx scripts/accessibility-check.ts",
    "insert-apartments": "tsx scripts/insertApartments.ts",
    "insert-categories": "tsx scripts/insertCategories.ts",
    "insert-users": "tsx scripts/insertUsers.ts",
    "clean": "rimraf .next node_modules",
    "analyze": "ANALYZE=true npm run build",
    "optimize": "tsx scripts/optimize.ts",
    "debug-netlify": "tsx scripts/debug-netlify.ts",
    "replace-sw-env": "tsx scripts/replace-sw-env.ts"
  },
